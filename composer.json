{
    "name": "i9code/shoppingcart",
    "description": "Laravel Shoppingcart",
    "keywords": ["laravel", "shoppingcart"],
    "license": "MIT",
    "authors": [
        {
            "name": "i9code",
            "email": "suportei9code@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "I9code\\Shoppingcart\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "I9code\\Tests\\Shoppingcart\\": "tests/"
        }
    },
    "require": {
        "laravel/framework": "~5.1.12|~5.2.0|~5.3.0|~5.4.0|~5.5.0|~5.6.0|~5.7.0|~5.8.0",
        "php": ">=5.5.9"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.0 || ~6.0 || ~7.0"
    },
    "suggest": {
        "i9code/notify": "Simple flash notifications for Laravel"
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "I9code\\Shoppingcart\\ShoppingcartServiceProvider"
            ],
            "aliases": {
                "Cart": "I9code\\Shoppingcart\\Facades\\Cart"
            }
        }
    }
}
